<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ts.ssm.dao.UserMapper">
    <select id="getUserById" resultType="User" parameterType="java.lang.Integer">
        SELECT
        *
        FROM user
        WHERE id = #{id}
    </select>
    <insert id="getUserById"  parameterType="User">
        INSERT INTO user (id, username, sex, date, age) VALUES (#{id}, #{username}, #{sex}, #{date}, #{age});
    </insert>
</mapper>